package src.Menus;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JOptionPane;

public class Commande_LoadGame extends JButton implements Commande, ActionListener{
	
	private JFrame fenetre;
	
	public Commande_LoadGame(JFrame fenetre) {
		super();
		this.fenetre = fenetre;
		
		//this.setText(this.getNom());
		//this.setFont(new Font("Arial", Font.BOLD, 30));
		//this.setForeground(Color.BLACK);
		
		this.setOpaque(false);
		this.setContentAreaFilled(false);
		
		this.addActionListener(this);
		
	}
	
	@Override
	public void executer() {
		JOptionPane.showMessageDialog(this.fenetre.getContentPane(), 
				"En cours de developpement");
	}

	@Override
	public String getNom() {
		return "Charger un partie";
	}

	@Override
	public void actionPerformed(ActionEvent arg0) {
		this.executer();		
	}
}
